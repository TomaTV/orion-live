# Base de données MySQL
DB_HOST=votre_host
DB_USER=nom_utilisateur
DB_PASSWORD=mot_de_passe_db
DB_NAME=nom_db

# Secret pour la gestion des sessions
SESSION_SECRET=sess_secret
JWT_SECRET=jwt_secret

# Configuration du cookie de session
COOKIE_NAME=auth
COOKIE_MAX_AGE=604800  # 7 jours en secondes

# Configuration pour l'environnement de production (si applicable)
NEXT_PUBLIC_API_URL=votreurl/api # Changez ceci pour l'URL de votre API en production
NEXT_PUBLIC_DISABLE_SOURCEMAPS=true

# Google OAuth
GOOGLE_ID=google_id
GOOGLE_SECRET=google_secret

GOOGLE_PSI_API_KEY=google_psi_api_key
SERP_API_KEY=serp_api_key

NEXTAUTH_URL=votreurl
NEXTAUTH_SECRET=nexauth_secret

#SMTP Setup
SMTP_HOST=smtp_host
SMTP_PORT=smtp_port
SMTP_USER=smtp_user
SMTP_PASS=smtp_pass
SMTP_FROM=smt_from

NEXT_PUBLIC_BASE_URL=next_public_base_url

# Security settings
BCRYPT_SALT_ROUNDS=12

# Désactiver SMTP en local pour éviter les erreurs SSL
SMTP_ENABLED=true
PASSWORD_RESET_EXPIRY=1800  # 30 minutes en secondes

# Rate limiting
RATE_LIMIT_WINDOW=3600000  # 1 heure en millisecondes
RATE_LIMIT_MAX_REQUESTS=5